
<form class="form-horizontal">
<h2>Inscription Entreprise</h2>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
<%= devise_error_messages! %>
<div class="form-group">
      <%= f.label :email , class:"control-label col-xs-2", style: "text-align:right" %>
      <div class="col-xs-10">
	  <%= f.email_field :email, autofocus: true ,class:"form-control inputEmail"%>
	  </div>
</div>
<div class="form-group">
	    <%= f.label :password , class:"control-label col-xs-2", style: "text-align:right"%>
	    <div class="col-xs-10">
		<%= f.password_field :password, autocomplete: "off",class:"form-control inputEmail" %>
	    </div>
</div>
<div class="form-group">
	    <%= f.label :password_confirmation , class:"control-label col-xs-2", style: "text-align:right" %>
	    <div class="col-xs-10">
		<%= f.password_field :password_confirmation, autocomplete: "off",class:"form-control inputEmail" %>
	    </div>
</div>
<div class="form-group">
	<div class="col-xs-offset-2 col-xs-10">
	<%= f.submit "Sign up" , class:"btn .btn_red", style:"height: 26px;padding-top: 2px;"%>
    </div>
</div>
<% end %>

</form>